<!DOCTYPE html>
<html>  
  <head>
    <title>Book</title>
    <link href="/assets/libraries_manifest.css" media="all" rel="stylesheet" type="text/css" />
  </head>
  <body>
    <div id="index_book">
      <h3>Listing Books</h3>
      <div class="hidden">
        <table class="book" cellpadding="10">
          <tr>
            <th>Name</th>
            <th>Link</th>
          </tr>
          <tr id="template">
            <td id="name"></td>
            <td id="show"></td>
            <td id="delete"></td>
          </tr>
        </table>
      </div>
    </div> 

    <div id="show_book">
      <div class="hidden">
        <table id="show_template"" cellpadding="10">
          <tr>
            <td>Name:</td> <td id="name"></td>
          </tr>
          <tr>
            <td>Code:</td> <td id="code"></td>
          </tr>
          <tr>
            <td>Author:</td> <td id="author"></td>
          </tr>
          <tr>
            <td>Price:</td> <td id="price"></td>
          </tr>
          <tr>
            <td>Publication:</td> <td id="publication"></td>
          </tr>
          <tr>
            <td>Version:</td> <td id="version"></td>
          </tr>
          <tr>
            <td id="back"></td> 
          </tr>
        </table>
      </div>
    </div>

  </body>
</html>

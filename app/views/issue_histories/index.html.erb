<!DOCTYPE html>
<html>  
  <head>
    <title>Issue History</title>
    <link href="/assets/libraries_manifest.css" media="all" rel="stylesheet" type="text/css" />
  </head>
  <body>
    <div id="index_ih">
      <h3>Listing Issue Histories</h3>
      <div class="hidden">
        <table class="issue_history" cellpadding="10">
          <tr>
            <th>Issue Date</th>
            <th>Link</th>
          </tr>
          <tr id="template">
            <td id="issue_date"></td>
            <td id="show"></td>
            <td id="delete"></td>
          </tr>
        </table>
      </div>
    </div> 

    <div id="show_ih">
      <div class="hidden">
        <table id="show_template"" cellpadding="10">
          <tr>
            <td>Issue Date:</td> <td id="issue_date"></td>
          </tr>
          <tr>
            <td>Return Date:</td> <td id="return_date"></td>
          </tr>
          <tr>
            <td>Copies:</td> <td id="copies"></td>
          </tr>
          <tr>
            <td id="back"></td> 
          </tr>
        </table>
      </div>
    </div>

  </body>
</html>

<!DOCTYPE html>
<html>  
  <head>
    <title>Member</title>
    <link href="/assets/libraries_manifest.css" media="all" rel="stylesheet" type="text/css" />
  </head>
  <body>
    <div id="index_mem">
      <h3>Listing Members</h3>
      <div class="hidden">
        <table class="member" cellpadding="10">
          <tr>
            <th>Name</th>
            <th>Link</th>
          </tr>
          <tr id="template">
            <td id="name"></td>
            <td id="show"></td>
            <td id="delete"></td>
          </tr>
        </table>
      </div>
    </div> 

    <div id="show_mem">
      <div class="hidden">
        <table id="show_template"" cellpadding="10">
          <tr>
            <td>Name:</td> <td id="name"></td>
          </tr>
          <tr>
            <td>Address:</td> <td id="address"></td>
          </tr>
          <tr>
            <td>Phone:</td> <td id="phone_no"></td>
          </tr>
          <tr>
            <td>Gender:</td> <td id="gender"></td>
          </tr>
          <tr>
            <td>Code:</td> <td id="code"></td>
          </tr>
          <tr>
            <td>Validity Date:</td> <td id="validity_date"></td>
          </tr>
          <tr>
            <td>Is Author:</td> <td id="is_author"></td>
          </tr>
          <tr>
            <td id="back"></td> 
          </tr>
        </table>
      </div>
    </div>

  </body>
</html>
